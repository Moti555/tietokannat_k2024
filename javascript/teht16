const prompt = require('prompt-sync')();

function myNumber() {
    let p1 = prompt("Anna 1. luku: ");
    let x = parseFloat(p1);
    let p2=prompt("Anna 2. luku: ");
    let y = parseFloat(p2);

    if(x > y)
        return x + " on suurempi";
    if (y > x)
        return y + " on suurempi";
    if ( x = y)
        return "Molemmat arvot ovat samat";
    else
        return "Virheellinen arvo";
}
console.log(myNumber());